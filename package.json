{
    "name": "wcgw",
    "displayName": "WCGW",
    "description": "Send code snippet and paths to Claude. Designed to work with wcgw mcp server.",
    "publisher": "AmanRusia",
    "repository": {
        "type": "git",
        "url": "https://github.com/rusiaaman/wcgw-vscode"
    },
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:wcgw.sendToApp"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "wcgw.sendToApp",
                "title": "WCGW: Send to Application"
            }
        ],
        "configuration": {
            "title": "WCGW",
            "properties": {
                "wcgw.targetApplication": {
                    "type": "string",
                    "default": "Claude",
                    "description": "Target application to send the code snippet to"
                }
            }
        },
        "keybindings": [
            {
                "command": "wcgw.sendToApp",
                "key": "cmd+'",
                "mac": "cmd+'",
                "when": "editorTextFocus && !inDebugRepl && !terminalFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
